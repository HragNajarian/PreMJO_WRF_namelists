#!/bin/bash
#SBATCH --job-name=job_real
#SBATCH --dependency=afterok:
#SBATCH -p radclouds
#SBATCH -N 1
#SBATCH -n 56
#SBATCH --nodelist=c795
#SBATCH --exclusive
#SBATCH --output=outbatch.out%j.txt
